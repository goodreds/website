{# <script type="text/javascript" src="/js/three.min.js" defer></script>
<script type="text/javascript" src="/js/vanta.fog.min.js" defer></script> #}
<script src="https://cdnjs.cloudflare.com/ajax/libs/three.js/r121/three.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/vanta@latest/dist/vanta.fog.min.js"></script>
<script>
VANTA.FOG({
  el: "#clouds",
  mouseControls: true,
  touchControls: true,
  gyroControls: false,
  minHeight: 200.00,
  minWidth: 200.00,
  highlightColor: 0xffd000,
  midtoneColor: 0xf8fcbd,
  lowlightColor: 0xfff449,
  baseColor: 0xffe99e,
  blurFactor: 0.90,
  speed: 0.30,
  zoom: 0.10
})
</script>

<script src="https://cdn.jsdelivr.net/npm/vanta@latest/dist/vanta.waves.min.js"></script>
<script>
VANTA.WAVES({
  el: "#waves",
  mouseControls: true,
  touchControls: true,
  gyroControls: false,
  minHeight: 200.00,
  minWidth: 200.00,
  scale: 1.00,
  scaleMobile: 1.00
})
</script>
